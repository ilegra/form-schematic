{
	"type": "object",
	"title": "Personal Informations",
	"properties": {
		"name": {
      "label": "Name",
      "inputType": "text",
			"maxLength": 80
		},
		"password": {
			"label": "Password",
			"inputType": "password",
			"minLength": 6
		},
		"email": {
      "label": "Email",
      "inputType": "email",
			"pattern": "^\\S+@\\S+$"
		},
		"age": {
			"label": "Age",
			"min": 0,
      "max": 120,
      "inputType": "number"
		},
		"nationality": {
			"label": "Nationality",
			"inputType": "text"
		},
		"cpf": {
			"label": "CPF",
			"inputType": "text",
			"mask": "000.000.000-00"
		},
		"phoneNumber": {
			"label": "Phone Number",
			"mask": "(00) 00000-0000",
			"inputType": "tel"
		}
	},
	"required": ["name", "cpf", "password", "email", "age"]
}
